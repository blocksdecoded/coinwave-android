<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FontTextView">
        <attr name="ftv_typeface" format="string">
            <enum name="thin" value="0"/>
            <enum name="light" value="1"/>
            <enum name="regular" value="2"/>
            <enum name="medium" value="3"/>
            <enum name="semibold" value="4"/>
            <enum name="bold" value="5"/>
            <enum name="heavy" value="6"/>
            <enum name="black" value="7"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="OptionSelectorView">
        <attr name="osv_options" format="string"/>
        <attr name="selectedNumBackground" format="integer"/>
        <attr name="defaultNumBackground" format="integer"/>
        <attr name="selectedTextColor" format="integer"/>
        <attr name="defaultTextColor" format="integer"/>
    </declare-styleable>
</resources>